  "scripts": {
    "build": "bob-esbuild build",
    "dev": "rollup -c --watch",
    "dev:all": "yarn dev:mjs:all",
    "dev:all:cjs": "yarn dev:cjs:all",
    "dev:all:mjs": "yarn dev:mjs:all",
    "dev:cjs:all": "pnpm build && concurrently npm:dev npm:dev:cjs:express npm:dev:cjs:fastify npm:dev:cjs:http npm:dev:cjs:koa npm:dev:cjs:hapi npm:dev:next",
    "dev:cjs:express": "cross-env PORT=3001 yarn --cwd ../../examples/app/express dev:cjs --silent",
    "dev:cjs:fastify": "cross-env PORT=3002 yarn --cwd ../../examples/app/fastify dev:cjs --silent",
    "dev:cjs:hapi": "cross-env PORT=3003 yarn --cwd ../../examples/app/hapi dev:cjs --silent",
    "dev:cjs:http": "cross-env PORT=3004 yarn --cwd ../../examples/app/http dev:cjs --silent",
    "dev:cjs:koa": "cross-env PORT=3005 yarn --cwd ../../examples/app/koa dev:cjs --silent",
    "dev:mjs:all": "pnpm build && concurrently npm:dev npm:dev:mjs:express npm:dev:mjs:fastify npm:dev:mjs:http npm:dev:mjs:koa npm:dev:mjs:hapi npm:dev:next",
    "dev:mjs:express": "cross-env PORT=3001 yarn --cwd ../../examples/app/express dev --silent",
    "dev:mjs:fastify": "cross-env PORT=3002 yarn --cwd ../../examples/app/fastify dev --silent",
    "dev:mjs:hapi": "cross-env PORT=3003 yarn --cwd ../../examples/app/hapi dev --silent",
    "dev:mjs:http": "cross-env PORT=3004 yarn --cwd ../../examples/app/http dev --silent",
    "dev:mjs:koa": "cross-env PORT=3005 yarn --cwd ../../examples/app/koa dev --silent",
    "dev:next": "yarn --cwd ../../examples/app/nextjs dev -p 3006",
    "prepare": "bob-esbuild build"
  },
