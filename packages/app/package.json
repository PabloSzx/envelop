    },
    "./koa": {
      "import": "./dist/esm/koa.js",
      "require": "./dist/cjs/koa.js"
    },
    "./nextjs": {
      "import": "./dist/esm/nextjs.js",
      "require": "./dist/cjs/nextjs.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc-node-build --clean",
    "dev": "tsc-node-watch",
    "dev:express": "tsc-node-build && concurrently --raw yarn:dev \"yarn --cwd ../../examples/app/express dev\"",
    "dev:fastify": "tsc-node-build && concurrently --raw yarn:dev \"yarn --cwd ../../examples/app/fastify dev\"",
    "dev:http": "tsc-node-build && concurrently --raw yarn:dev \"yarn --cwd ../../examples/app/http dev\"",
    "dev:koa": "tsc-node-build && concurrently --raw yarn:dev \"yarn --cwd ../../examples/app/koa dev\"",
    "dev:next": "tsc-node-build && concurrently --raw yarn:dev \"yarn --cwd ../../examples/app/nextjs dev\"",
    "prepare": "tsc-node-build --clean"
  },
