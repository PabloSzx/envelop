  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0",
    "graphql-helix": "^1.2.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
