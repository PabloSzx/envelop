{
  "name": "example-app-fastify",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "tsup": {
    "splitting": false,
    "sourcemap": true,
    "entryPoints": [
      "src/index.ts"
    ]
  },
  "scripts": {
    "dev": "yarn dev:mjs",
    "dev:cjs": "tsup-node --format cjs --watch src --onSuccess \"node dist/index.js\"",
    "dev:mjs": "tsup-node --format esm --watch src --onSuccess \"node dist/index.mjs\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.1.0",
    "@pablosz/envelop-app": "workspace:^",
    "fastify": "^3.15.1",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "tsup": "^4.11.0",
    "typescript": "^4.2.4"
  }
}
