{
  "name": "example-app-fastify",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "yarn dev:mjs",
    "dev:cjs": "tsup-node src/index.ts --format cjs --sourcemap --no-splitting --watch src --onSuccess \"node dist/index.js\"",
    "dev:mjs": "tsup-node src/index.ts --format esm --sourcemap --no-splitting --watch src --onSuccess \"node dist/index.mjs\"",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@envelop/app": "^0.1.0",
    "@graphql-typed-document-node/core": "^3.1.0",
    "fastify": "^3.15.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "tsup": "^4.10.0",
    "typescript": "^4.2.4"
  }
}
